{
    "redirects": [
        {
            "source": "/(.*)",
            "destination": "https://gsaloumrane.vercel.app/",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "functions": {
        "api/generate.ts": {
            "memory": 1024,
            "maxDuration": 30
        }
    }
}